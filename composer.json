{
	"name": "gandung/tokopedia-php-sdk-bundle",
	"type": "symfony-bundle",
	"description": "Symfony bundle module for Tokopedia PHP SDK.",
	"keywords": [
		"tokopedia",
		"tokopedia-api",
		"tokopedia-sdk",
		"tokopedia-sdk-api",
		"tokopedia-api-sdk",
		"symfony-bundle",
		"tokopedia-sdk-php-bundle",
		"bundle"
	],
	"license": "BSD-3-Clause",
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Gandung\\Tokopedia\\TokopediaBundle\\": ""
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gandung\\Tokopedia\\TokopediaBundle\\Tests\\": "Tests/"
		}
	},
	"require": {
		"php": "^7.1",
		"gandung/tokopedia-php-sdk": "^1.0@dev",
		"symfony/config": "^5.1",
		"symfony/dependency-injection": "^5.1",
		"symfony/http-kernel": "^5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.4@dev",
		"symfony/cache": "^5.2@dev"
	}
}
